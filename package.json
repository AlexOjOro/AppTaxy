{
  "name": "template",
  "version": "1.0.0",
  "scripts": {
    "start": "start-storybook -p 4200 --ci",
    "build": "build-storybook -o dist",
    "test": "ng test --watch false --code-coverage",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "stylelint": "stylelint src/**/*.{scss,html}",
    "stylelint:fix": "stylelint src/**/*.{scss,html} --fix",
    "prettier": "prettier --check \"**/*.{md,mdx,ts,js,scss,html,json,yml}\" --ignore-path .gitignore",
    "prettier:fix": "prettier --write \"**/*.{md,mdx,ts,js,scss,html,json,yml}\" --ignore-path .gitignore"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^9.0.0",
    "@angular/flex-layout": "^9.0.0-beta.31",
    "@angular/material": "^9.0.0",
    "@jabil/ui-ng": "^0.1.3",
    "@jabil/ui-styles": "^0.1.8",
    "@ng-select/ng-select": "^5.0.9",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@popperjs/core": "^2.5.4",
    "@swimlane/ngx-datatable": "^15.0.2",
    "bootstrap": "^4.5.3",
    "hammerjs": "^2.0.8",
    "jquery": "^3.5.1",
    "microsoft-adal-angular6": "^1.3.0",
    "ngx-bootstrap": "^5.6.2",
    "ngx-color-picker": "^8.2.0",
    "ngx-loading": "^8.0.0",
    "ngx-toastr": "^11.3.3",
    "perfect-scrollbar": "^1.5.0",
    "primeng": "^9.0.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.14.1",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.12",
    "@angular/animations": "~9.1.0",
    "@angular/cli": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-a11y": "~6.0.0",
    "@storybook/addon-actions": "~6.0.0",
    "@storybook/addon-backgrounds": "~6.0.0",
    "@storybook/addon-docs": "~6.0.0",
    "@storybook/addon-knobs": "~6.0.0",
    "@storybook/addon-links": "~6.0.0",
    "@storybook/addon-viewport": "~6.0.0",
    "@storybook/addons": "~6.0.0",
    "@storybook/angular": "~6.0.0",
    "@storybook/theming": "~6.0.0",
    "@types/jasmine": "^3.0.0",
    "@types/jasminewd2": "^2.0.0",
    "@types/node": "^12.0.0",
    "babel-loader": "^8.0.0",
    "classlist.js": "1.1.20150312",
    "codelyzer": "^6.0.0",
    "husky": "^4.3.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^5.0.0",
    "karma-chrome-launcher": "^3.0.0",
    "karma-coverage-istanbul-reporter": "^3.0.0",
    "karma-jasmine": "^4.0.0",
    "karma-jasmine-html-reporter": "^1.0.0",
    "karma-junit-reporter": "^2.0.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.0.0",
    "rxjs": "~6.6.0",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-rational-order": "^0.1.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.0.0",
    "ts-node": "~9.0.0",
    "tslib": "~1.14.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.0.0",
    "typescript": "~3.8.0",
    "web-animations-js": "^2.0.0",
    "zone.js": "^0.10.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit \"\"",
      "pre-commit": "pretty-quick --staged --pattern \"**/*.{md,mdx,ts,js,scss,html,json,yml}\""
    },
    "skipCI": false
  },
  "browserslist": [
    "IE 11",
    "defaults"
  ],
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "overrides": [
      {
        "files": [
          "*.json",
          "*.md",
          "*.yml"
        ],
        "options": {
          "tabWidth": 2,
          "useTabs": false
        }
      }
    ],
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "all",
    "useTabs": true
  },
  "stylelint": {
    "extends": [
      "stylelint-config-standard",
      "stylelint-config-rational-order",
      "stylelint-config-prettier"
    ],
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "at-rule-no-unknown": null,
      "at-rule-no-vendor-prefix": true,
      "media-feature-name-no-vendor-prefix": true,
      "no-empty-source": null,
      "property-no-vendor-prefix": true,
      "selector-no-vendor-prefix": true,
      "selector-pseudo-element-colon-notation": "single",
      "selector-pseudo-element-no-unknown": [
        true,
        {
          "ignorePseudoElements": [
            "ng-deep"
          ]
        }
      ],
      "selector-type-no-unknown": null,
      "value-no-vendor-prefix": true,
      "scss/at-rule-no-unknown": true
    }
  }
}
